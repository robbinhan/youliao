<?xml version='1.0' encoding='utf-8'?>
<widget id="com.robbinhan.Youliao" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <icon height="60" src="icons/ios/icon.jpg" width="60" />
    <icon height="120" src="icons/ios/icon.jpg" width="120" />
    <icon height="76" src="icons/ios/icon.jpg" width="76" />
    <icon height="152" src="icons/ios/icon.jpg" width="152" />
    <icon height="40" src="icons/ios/icon.jpg" width="40" />
    <icon height="80" src="icons/ios/icon.jpg" width="80" />
    <icon height="57" src="icons/ios/icon.jpg" width="57" />
    <icon height="114" src="icons/ios/icon.jpg" width="114" />
    <icon height="72" src="icons/ios/icon.jpg" width="72" />
    <icon height="144" src="icons/ios/icon.jpg" width="144" />
    <icon height="29" src="icons/ios/icon.jpg" width="29" />
    <icon height="58" src="icons/ios/icon.jpg" width="58" />
    <icon height="50" src="icons/ios/icon.jpg" width="50" />
    <icon height="100" src="icons/ios/icon.jpg" width="100" />
    <name>Youliao</name>
    <description>
        stackexchange、segmentfault、stackoverflow、hacknews、startupnews的文章聚合器
    </description>
    <author email="luckyhanbin@gmail.com" href="http://robbinhan.logdown.com/">
        Robbin Han
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
</widget>
