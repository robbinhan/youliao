<?xml version='1.0' encoding='utf-8'?>
<widget id="com.robbinhan.Youliao" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Youliao</name>
    <description>
        stackexchange、segmentfault、stackoverflow、hacknews、startupnews的文章聚合器
    </description>
    <author email="luckyhanbin@gmail.com" href="http://robbinhan.logdown.com/">
        Robbin Han
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <platform name="ios">

      <!-- iOS 7.0+ -->
      <!-- iPhone / iPod Touch -->
      <icon src="icons/ios/icon.jpg" width="60" height="60" />
      <icon src="icons/ios/icon.jpg" width="120" height="120" />

      <!-- iPad -->
      <icon src="icons/ios/icon.jpg" width="76" height="76" />
      <icon src="icons/ios/icon.jpg" width="152" height="152" />

      <!-- iOS 6.1 -->
      <!-- Spotlight Icon -->
      <icon src="icons/ios/icon.jpg" width="40" height="40" />
      <icon src="icons/ios/icon.jpg" width="80" height="80" />

      <!-- iPhone / iPod Touch -->
      <icon src="icons/ios/icon.jpg" width="57" height="57" />
      <icon src="icons/ios/icon.jpg" width="114" height="114" />

      <!-- iPad -->
      <icon src="icons/ios/icon.jpg" width="72" height="72" />
      <icon src="icons/ios/icon.jpg" width="144" height="144" />

      <!-- iPhone Spotlight and Settings Icon -->
      <icon src="icons/ios/icon.jpg" width="29" height="29" />
      <icon src="icons/ios/icon.jpg" width="58" height="58" />

      <!-- iPad Spotlight and Settings Icon -->
      <icon src="icons/ios/icon.jpg" width="50" height="50" />
      <icon src="icons/ios/icon.jpg" width="100" height="100" />
    </platform>
</widget>
